      </Label>
      <TextField fx:id="priceTextField" layoutX="80.0" layoutY="451.0" prefHeight="26.0" prefWidth="482.0" />
      <Button fx:id="generateBillButton" layoutX="80.0" layoutY="521.0" mnemonicParsing="false" onAction="#generateBillButtonOnAction" prefHeight="26.0" prefWidth="482.0" style="-fx-background-color: #000000;" text="Generate Bill" textFill="#f2eaea">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="closeButton" layoutX="80.0" layoutY="568.0" mnemonicParsing="false" onAction="#closeButtonOnAction" prefHeight="26.0" prefWidth="482.0" style="-fx-background-color: #000000;" text="Close" textFill="#f2eaea">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Label fx:id="confirmLabel" alignment="CENTER" layoutY="491.0" prefHeight="18.0" prefWidth="602.0" />
