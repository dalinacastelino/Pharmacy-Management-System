<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="718.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.pharmacy_management_system.Dashboard">
   <children>
      <ImageView fx:id="pharmacyImageView" fitHeight="500.0" fitWidth="496.0" pickOnBounds="true">
         <image>
            <Image url="@../../../../../../../../OneDrive/Pictures/pharmacy_dashboard3.jpg" />
         </image>
      </ImageView>
      <AnchorPane layoutX="496.0" prefHeight="62.0" prefWidth="224.0" style="-fx-background-color: #000000;">
         <children>
            <Label alignment="CENTER" layoutX="27.0" layoutY="17.0" prefHeight="18.0" prefWidth="170.0" text="DASHBOARD" textFill="#f4eeee">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Button fx:id="searchMedicinesButton" layoutX="509.0" layoutY="94.0" mnemonicParsing="false" onAction="#searchMedicinesOnAction" prefHeight="30.0" prefWidth="197.0" style="-fx-background-color: #000000;" text="Search Medicines" textFill="#f4e9e9">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="addMedicinesButton" layoutX="509.0" layoutY="235.0" mnemonicParsing="false" onAction="#addMedicinesButtonOnAction" prefHeight="30.0" prefWidth="197.0" style="-fx-background-color: #000000;" text="Add New Medicines" textFill="#f4e9e9">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="billingButton" layoutX="509.0" layoutY="143.0" mnemonicParsing="false" onAction="#billingButtonOnAction" prefHeight="30.0" prefWidth="197.0" style="-fx-background-color: #000000;" text="Billing" textFill="#f4e9e9">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="updateQuantityButton" layoutX="509.0" layoutY="287.0" mnemonicParsing="false" onAction="#updateQuantityOnAction" prefHeight="30.0" prefWidth="197.0" style="-fx-background-color: #000000;" text="Update Quantity" textFill="#f4e9e9">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="orderDetailsButton" layoutX="509.0" layoutY="340.0" mnemonicParsing="false" onAction="#orderDetailsOnAction" prefHeight="30.0" prefWidth="197.0" style="-fx-background-color: #000000;" text="Order Details" textFill="#f4e9e9">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="closeButton" layoutX="509.0" layoutY="434.0" mnemonicParsing="false" onAction="#closeButtonOnAction" prefHeight="30.0" prefWidth="197.0" style="-fx-background-color: #000000;" text="Close" textFill="#f4e9e9">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
